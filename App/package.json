{
  "name": "helpital_app",
  "productName": "helpital_app",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "views/main.js",
  "scripts": {
    "sass": "node-sass views/assets/scss -o views/assets/css",
    "electron": "electron .",
    "start": "npm run-script sass && npm run-script electron",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "test": "mocha"

  },
  "keywords": [],
  "author": {
    "name": "VHouze",
    "email": "victor.houze@epitech.eu"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "helpital_app"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.3",
    "connect-flash": "^0.1.1",
    "consolidate": "^0.16.0",
    "ejs": "^3.1.5",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.17.1",
    "express-ejs-layouts": "^2.5.0",
    "express-session": "^1.17.1",
    "fs": "0.0.1-security",
    "jquery": "^3.5.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.3",
    "node-sass": "^5.0.0",
    "swig": "^1.4.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "electron": "11.0.3"
  }
}
